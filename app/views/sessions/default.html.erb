<div class="error">
<% if flash[:errors] %>
<%= flash[:errors] %>
<% end %>
<% if flash[:message] %>
<%= flash[:message] %>
<% end %>
</div>

<div class="container">
<%= form_tag(loggin_path, method: :get) do %>

<%= label_tag :user_name %>
<%= text_field_tag :user_name %>

<%= label_tag :password %>
<%= password_field_tag :password %>

<%= submit_tag "Log-in"%>
<% end %>
</div>