<h1>Add a Photo</h1>

<%= form_for @photo do |f| %>

    <%= f.label :title, "Title of your Photo" %>
    <%= f.text_field :title %>

    <%= f.label :image, "Upload The Image" %>
    <%= f.file_field :image %>

    <%= f.label :location_id, "Select Your Location" %>
    <%= f.collection_select :location_id, @locations, :id, :name  %>

    <%= f.submit %>

<% end %>